{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Risks",
  "type": "object",
  "description": "A list of potential risks or concerns extracted from a text.",
  "properties": {
    "risks": {
      "type": "array",
      "items": {
        "type": "string",
        "description": "A potential risk or concern."
      }
    }
  },
  "required": ["risks"]
}
