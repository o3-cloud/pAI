{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RetroSummary",
  "type": "object",
  "properties": {
    "what_worked": { "type": "array", "items": { "type": "string" }, "description": "Things that worked well during the period." },
    "what_didnt": { "type": "array", "items": { "type": "string" }, "description": "Things that did not work well during the period." },
    "actions": { "type": "array", "items": { "type": "string" }, "description": "Action items identified for improvement." }
  },
  "required": ["what_worked", "what_didnt"]
}
